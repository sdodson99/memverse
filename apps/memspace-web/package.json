{
  "name": "memspace-web",
  "version": "1.2.0",
  "description": "Web application for interacting with Memspace user messages.",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:stage": "NODE_ENV=test next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint:eslint": "eslint ./src --ext=.tsx,.ts",
    "lint:prettier": "prettier --check ./src",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "fix:eslint": "eslint --fix ./src --ext=.tsx,.ts",
    "fix:prettier": "prettier --write ./src",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "export": "next export",
    "generate": "npx generate-react-cli component",
    "deploy": "firebase deploy --only hosting",
    "deploy:stage": "firebase hosting:channel:deploy --expires 7d"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@next/bundle-analyzer": "^12.1.5",
    "axios": "^0.24.0",
    "constate": "^3.3.0",
    "firebase": "^9.6.6",
    "framer-motion": "^6.3.3",
    "luxon": "^2.3.0",
    "next": "12.0.7",
    "paper": "^0.12.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.27.1",
    "react-paginate": "^8.1.3",
    "react-query": "^3.34.16",
    "react-spring-bottom-sheet": "^3.4.0",
    "recoil": "^0.7.2",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.16.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.23",
    "@types/jest-when": "^2.7.4",
    "@types/luxon": "^2.0.8",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "firebase-tools": "^10.2.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.4",
    "jest-canvas-mock": "^2.3.1",
    "jest-when": "^3.5.0",
    "prettier": "^2.3.1",
    "typescript": "4.5.4"
  }
}

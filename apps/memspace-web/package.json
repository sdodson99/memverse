{
  "name": "memspace-web",
  "version": "1.0.0",
  "description": "Web application for interacting with Memspace user messages.",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5500",
    "build": "next build",
    "start": "next start",
    "lint:eslint": "eslint ./src --ext=.tsx,.ts",
    "lint:prettier": "prettier --check ./src",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "fix:eslint": "eslint --fix ./src --ext=.tsx,.ts",
    "fix:prettier": "prettier --write ./src",
    "fix": "npm run fix:eslint && npm run fix:prettier"
  },
  "dependencies": {
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-gapi-auth2": "^1.0.4"
  },
  "devDependencies": {
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.1",
    "typescript": "4.5.4"
  }
}
